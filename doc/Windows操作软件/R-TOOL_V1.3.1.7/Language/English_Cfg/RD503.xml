        <TreeItem Type="PropTreeItem" Des="Block's parameters">
          <TreeItem Type="PropTreeItemEdit"  Des="Block address" ValueFormat="ValueFormatNumber" MaxValue="0xFF" StartBitPos="3.3.0" EndBitPos="3.3.7"/>
