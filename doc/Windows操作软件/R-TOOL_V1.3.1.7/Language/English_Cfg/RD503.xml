    </TreeItem>

    <TreeItem Type="PropTreeItem" Des="Trigger options">
      <TreeItem Type="PropTreeItemCheckBox" Des="Close RF when free automatically" StartBitPos="3.0.2" EndBitPos="3.0.2" CheckValue="0x01" UnCheckValue="0x00"/>
    </TreeItem>

    <TreeItem Type="PropTreeItem" Des="Default output port setting">
