            <List Des="New line(0x0A)" Value="0x01"/>
            <List Des="Enter(0x0D)" Value="0x02"/>
            <List Des="TAB(0x07)" Value="0x03"/>
            <List Des="Semicolon(0x3B)" Value="0x04"/>
            <List Des="Comma(0x2C)" Value="0x05"/>
